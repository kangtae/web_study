<% 
  let gnb = locals.require('gnb.json')
  var page = 'notice'
  let sub = '목록'
  let heading = (sub && (gnb[page].children[sub].hasOwnProperty('view') === false || gnb[page].children[sub].view !== false )) ? ((gnb[page].children[sub].hasOwnProperty('title') === false || gnb[page].children[sub].title == "" ) ? sub : gnb[page].children[sub].title) : ((gnb[page].children[sub].hasOwnProperty('title') === false || gnb[page].children[sub].title == "" ) ? gnb[page].title.ko : gnb[page].children[sub].title)
-%>
<% locals.setLayout('_base.ejs') -%>
<% locals.block('page',page) %>
<% locals.block('sub',sub) %>
<% locals.block('heading',heading) %>
<% locals.block('title', heading + ' | ','append') -%>


<div class="row">
  <div class="col-sm-12">
    
    <table class="table table-striped table-bordered">
      <thead>
        <th class="text-center" style="width:60px;">번호</th>
        <th class="text-center">제목</th>
        <th class="text-center" style="width:100px;">등록일</th>
        <th class="text-center" style="width:100px;">조회수</th>
      </thead>
      <tbody>
        <% for (var i=10; i > 0; i--) { %>
          <tr>
            <td class="text-center"><%- i %></td>
            <td><a href="<%- gnb[page].children['상세'].link %>.html" class="board-link board-nowrap">현대아산, 미국 프리미엄 탄산수 크리스탈 가이저(Crystal Geyser) 출시</a></td>
            <td class="text-center">2018-11-21</td>
            <td class="text-center">275</td>
          </tr>
        <% } -%>
      </tbody>
    </table>
    <!-- //table -->
  </div>
</div>

<div class="row">
  <div class="col-xs-12">
    <%- locals.widget('_pagination.ejs') %>
  </div>
</div>

<div class="row">
  <div class="col-sm-4 text-left">
  </div>
  <div class="col-sm-4 text-center"></div>
  <div class="col-sm-4 text-right">
    <a class="btn btn-success" href="<%- gnb[page].children['등록'].link %>.html" role="button"><i class="fa fa-pencil" aria-hidden="true"></i> 등록</a>
  </div>
</div>